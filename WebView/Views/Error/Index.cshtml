@{
    Layout = "_LayoutError";
}


<div id="notfound">
    <div class="notfound">
        <div class="notfound-404">
            <h1>@ViewData["Code"]</h1>
        </div>
        <h2>@ViewData["Title"]!</h2>
        <p>@ViewData["Message"].</p>
        <div class="d-flex align-items-center justify-content-around mt-2">
            <a onclick="backToHistory()">Quay lại</a>
            <a onclick="backToDashboard()">Trở về trang chủ</a>
        </div>
    </div>
</div>
@section Scripts{
    <script>
        function backToDashboard() {
            window.location.href = "/"
        }
        function backToHistory(){
            window.history.back();
        }
    </script>
}
